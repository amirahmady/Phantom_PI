    "version": "0.2.0",
    "configurations": [
        
    
    
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Phantom_pi, pcan",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args" : ["-c","pcan"]
        },
        {
            "name": "Python: Phantom_pi, pcan, refrence search on",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args" : ["-c","pcan","-r","3","--add-axis","2 3 4 .10 5 6 .05"],
        },
        {
            "name": "Python: Phantom_pi, pcan, init and rfs on",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args" : ["-c","pcan","-r","3","-i","+35"]
        },
        {
            "name": "Python: Current File (External Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal"
        }
    ]
}